<h1>Signup</h1>

<%= form_for @user do |f| %>
  <%= f.label_for :username %>
  <%= f.text_field :username %>

  <%= f.label_for :password %>
  <%= f.password_field :password %>

  <%= f.label_for :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit %>
<% end %>
